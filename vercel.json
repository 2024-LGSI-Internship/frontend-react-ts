{
  "rewrites": [
    {
      "source": "/settings/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/settings/:path*"
    },
    {
      "source": "/settings/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/settings/:path*/"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/dashboard/:path*"
    },
    {
      "source": "/dashboard/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/dashboard/:path*/"
    },
    {
      "source": "/chat/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/chat/:path*/"
    },
    {
      "source": "/chat/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/chat/:path*"
    },
    {
      "source": "/img/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/img/:path*"
    },
    {
      "source": "/img/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/img/:path*/"
    }
  ]
}