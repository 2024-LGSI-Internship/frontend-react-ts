{
  "rewrites": [
    {
      "source": "/settings/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/settings/:path*"
    },
    {
      "source": "/settings/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/settings/:path*/"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "https://lgsi-backend-vercel.vercel.app/dashboard/:path*"
    },
    {
      "source": "/dashboard/:path*/",
      "destination": "https://lgsi-backend-vercel.vercel.app/dashboard/:path*/"
    },
    {
      "source": "/chat",
      "destination": "https://lgsi-backend-vercel.vercel.app/chat"
    },
    {
      "source": "/chat/",
      "destination": "https://lgsi-backend-vercel.vercel.app/chat"
    },
    {
      "source": "/img",
      "destination": "https://lgsi-backend-vercel.vercel.app/img"
    },
    {
      "source": "/img/",
      "destination": "https://lgsi-backend-vercel.vercel.app/img"
    }
  ]
}